programa {
  funcao inicio() {
     inteiro nota1, nota2, nota3, media

    escreva("Digite sua primeira nota : ")
    leia (nota1)
    escreva("Digite sua segunda nota : ")
    leia (nota2)
    escreva ("Agora digite sua terceira nota : ")
    leia (nota3)

    media = (nota1+nota2+nota3) / 3

    escreva("Sua média é : ", media)

  }
}