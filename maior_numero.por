programa {
  funcao inicio() {

    inteiro num1, num2

    escreva ("Informe um número : " )
    leia (num1)
    escreva ("Informe o segundo número : ")
    leia (num2)

    se (num1>num2){
       escreva ("O primeiro número informado é maior.")
       }
    senao {
       escreva (" O segundo número informado é maior.")
       }

  }
}

